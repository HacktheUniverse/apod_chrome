{
  "manifest_version": 2,

  "name": "Astronomy Picture of the Day",
  "description": "This extension displays the Astronomy Picture of the Day from NASA's website.",
  "version": "1.0",

  "chrome_url_overrides" : {
    "newtab" : "apod.html"
  },
  "permissions": [ "tabs", "http://www.google.com/" ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["style.css"],
      "js": ["jquery.min.js", "script.js"]
    }
  ]
}